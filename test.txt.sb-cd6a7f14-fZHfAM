\f0\fs24 \cf0 --------------------------------------------------------------   \
with su as\
 (select cl.localsystemid, sub.client_localsystemid, cl.version\
  from client cl, subscriber sub\
  where 1 = 1\
        and cl.version = 0\
        and cl.localsystemid = sub.client_localsystemid(+)\
        and rownum < 20)\
select * from su \
where client_localsystemid is null\
\
\
----------------------------------\
select *\
from (select v.dtype, v.localsystemid, v.version  from client v)  t\
where\
t.localsystemid='100000087065'\
and t.version=0\
-------------------------------------------\
  \
with vit as\
 (select v.dtype, v.localsystemid, v.version  from client v) \
select * from vit\
where localsystemid='100000087065'\
and version=0\
\
------------------------------------------}